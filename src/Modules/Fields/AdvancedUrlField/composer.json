{
    "name": "webid/advanced-url-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Webid\\AdvancedUrlField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webid\\AdvancedUrlField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
