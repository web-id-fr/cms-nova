{
    "name": "webid/cms-nova",
    "description": "CMS for Laravel with Nova",
    "autoload": {
        "psr-4": {
            "Webid\\CmsNova\\": "src/",
            "Webid\\CmsNova\\App\\": "src/app/",
            "Webid\\SwitchLanguage\\" : "src/Modules/SwitchLanguage/src/",
            "Webid\\LanguageTool\\" : "src/Modules/LanguageTool/src/",
            "Webid\\MenuTool\\" : "src/Modules/MenuTool/src/",
            "Webid\\ComponentTool\\" : "src/Modules/ComponentTool/src/",
            "Webid\\AdvancedUrlField\\": "src/Modules/Fields/AdvancedUrlField/src/",
            "Webid\\FieldItemField\\" : "src/Modules/Fields/FieldItemField/src/",
            "Webid\\MenuItemField\\" : "src/Modules/Fields/MenuItemField/src/",
            "Webid\\ComponentItemField\\": "src/Modules/Fields/ComponentItemField/src/",
            "Webid\\ServiceItemField\\": "src/Modules/Fields/ServiceItemField/src/",
            "Webid\\PageUrlItemField\\" : "src/Modules/Fields/PageUrlItemField/src/",
            "Webid\\TemplateItemField\\" : "src/Modules/Fields/TemplateItemField/src/",
            "Webid\\ImageItemField\\" : "src/Modules/Fields/ImageItemField/src/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "LIEGEOIS Elise",
            "email": "elise@web-id.fr"
        },
        {
            "name": "JOURDIL Wilfried",
            "email": "wilfried@web-id.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9|^10",
        "laravel/nova": "~4.0",
        "spatie/laravel-honeypot": "^4.3",
        "nwidart/laravel-modules": "10.*",
        "oneduo/nova-file-manager": "^0.12.0",
        "alexwenzel/nova-dependency-container": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^7.4",
        "khaled-dev/nova-assertions": "^1.2",
        "nunomaduro/larastan": "^1.0.2",
        "barryvdh/laravel-ide-helper": "^2.13",
        "laravel/pint": "^1.0",
        "phparkitect/phparkitect": "^0.3.25",
        "enlightn/security-checker": "^1.10"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/app/",
            "Webid\\ComponentTool\\" : "src/Modules/ComponentTool/src/"
        }
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=4G",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf || true",
        "pint": "pint",
        "style:fix": [
            "@pint",
            "@phpcbf"
        ],
        "phparkitect:check": "phparkitect check",
        "security-checker": "security-checker security:check"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nwidart\\Modules\\LaravelModulesServiceProvider",
                "Laravel\\Nova\\NovaServiceProvider",
                "Webid\\CmsNova\\CmsServiceProvider",
                "Webid\\LanguageTool\\ToolServiceProvider",
                "Webid\\AdvancedUrlField\\FieldServiceProvider",
                "Webid\\MenuTool\\ToolServiceProvider",
                "Webid\\ComponentItemField\\FieldServiceProvider",
                "Webid\\MenuItemField\\FieldServiceProvider",
                "Webid\\ServiceItemField\\FieldServiceProvider",
                "Webid\\FieldItemField\\FieldServiceProvider",
                "Webid\\PageUrlItemField\\FieldServiceProvider",
                "Webid\\TemplateItemField\\FieldServiceProvider",
                "Webid\\ComponentTool\\ToolServiceProvider",
                "Webid\\SwitchLanguage\\ToolServiceProvider",
                "Webid\\ImageItemField\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./cms-nova/src/nova-components/ComponentItemField"
        }
    ]
}
